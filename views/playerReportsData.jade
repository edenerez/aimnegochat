extends shared

block content
 div.container
   h1 Player #{player} Information (#{gametype.split('_')[1]})
   br

   h2 User Info
   div.scrol
     table(class="table table-striped table-bordered")
       tr
         th Game Id
         th Type
         th Role
       - if (playerList !== undefined)
         each player in playerList 
          tr
            td #{player.PartitionKey}
            td #{player.type}
            td #{player.role}
   br

   h2 Pre Questionnaire
   div.scrol
     table(class="table table-striped table-bordered")
       tr
         th Gender
         th Age
         th Education
         th Field
         th Birth Country
         th browser Type
         th assisnmentid
         th hitid
         th workerid
       - if (questionnaireList !== undefined) 
         each questionnaire in questionnaireList
          tr
            td #{questionnaire.gender}
            td #{questionnaire.age}
            td #{questionnaire.education}
            td #{questionnaire.field}
            td #{questionnaire.birth_country}
            td #{questionnaire.browserType}
            td #{questionnaire.assisnmentid}
            td #{questionnaire.hitid}
            td #{questionnaire.workerid}
  
   h2 Post Questionnaire
   div.scrol
     table(class="table table-striped table-bordered")
       tr
         th computer_program
         th fairness
         th feedback
         th happy
         th instruction
         th otherGender
         th same_country
         th type
       - if (questionnaireList !== undefined) 
         each questionnaire in questionnaireList
          tr
            td #{questionnaire.computer_program}
            td #{questionnaire.fairness}
            td #{questionnaire.feedback}
            td #{questionnaire.happy}
            td #{questionnaire.instruction}
            td #{questionnaire.otherGender}
            td #{questionnaire.same_country}
            td #{questionnaire.type}