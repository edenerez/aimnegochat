html(lang='en')
head
    meta(charset='utf-8')
    title Chat-Chat
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(name='description', content='')
    meta(name='author', content='')
    //
       Le styles 
    link(href='../assets/css/bootstrap.css', rel='stylesheet')
    style(type='text/css')
      body {
      padding-top: 60px;
      padding-bottom: 40px;
      }
    link(href='../assets/css/bootstrap-responsive.css', rel='stylesheet')
    //
       HTML5 shim, for IE6-8 support of HTML5 elements 
    //if lt IE 9
      script(src='../assets/js/html5shiv.js')
    //
       Fav and touch icons 
    link(rel='apple-touch-icon-precomposed', sizes='144x144', href='../assets/ico/apple-touch-icon-144-precomposed.png')
    link(rel='apple-touch-icon-precomposed', sizes='114x114', href='../assets/ico/apple-touch-icon-114-precomposed.png')
    link(rel='apple-touch-icon-precomposed', sizes='72x72', href='../assets/ico/apple-touch-icon-72-precomposed.png')
    link(rel='apple-touch-icon-precomposed', href='../assets/ico/apple-touch-icon-57-precomposed.png')
    link(rel='shortcut icon', href='../assets/ico/favicon.png')
  body
    .navbar.navbar-inverse.navbar-fixed-top
      .navbar-inner
        .container
          button.btn.btn-navbar(type='button', data-toggle='collapse', data-target='.nav-collapse')
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.brand(href='#') Chat Project
          .nav-collapse.collapse
            ul.nav
              li
                a(href='/') Home
              for type in gametypes  
                  if (type == gametype) 
                    li.active
                      a(href='/#{type}/gametype') #{type}
                  else
                    li
                      a(href='/#{type}/gametype') #{type}
          //
            /.nav-collapse 
    .container
      
      h1 BIU AI lab gametype server - Main Page
      h4
          a(href='/users') Active users
          div(style='background-color: #eee;')
            h1 #{gametype} Lists
            h4
              a(href='/#{gametype}/listAllQuestionnaire') All Questionnaire
            h4
              a(href='/#{gametype}/listAllGames') All Games
            h4
              a(href='/#{gametype}/listAllGameAction') All Games Actions              
            h4
              a(href='/#{gametype}/listAllFinalResults') All FinalResults 
            h4
              a(href='/#{gametype}/listNewKBAgentInit') New KBAgent init
            h4
              a(href='/#{gametype}/listactive') Active games              
            h4
              a(href='/#{gametype}/listlogs') Logged games
            h4
              a(href='/#{gametype}/listlogs?finished=1') Finished games
            h4
              a(href='/#{gametype}/listlogs?pretty=1') Verified games
            h4
              a(href='/LogToXml/PreQuestionnaireDemography') answers to demographic questionnaire
            h4
              a(href='/LogToXml/PostQuestionnaire') answers to post-gametype questionnaire
            hr
            h2 Let's Play!
            h4
              | note: Open different links in different browsers, to make the server think they are different users.
            h4
              a(href='/#{gametype}/beginner') PLAY - A worker not from Amazon Turk
            h4
              a(href='/#{gametype}/beginner?assignmentId=ASSIGNMENT_ID_NOT_AVAILABLE') PLAY - Amazon Turk Preview mode
            h4
              a(href='/#{gametype}/beginner?workerId=amazonworker123&hitId=hit123&assignmentId=assignment123&turkSubmitTo=sandbox') PLAY - Amazon Turk Live mode with a dummy worker
            h4
              a(href='/#{gametype}/advanced') PLAY - A worker not from Amazon Turk - jump directly to game
            h4
              a(href='/#{gametype}/advanced?workerId=amazonworker123&hitId=hit123&assignmentId=assignment123&turkSubmitTo=sandbox') PLAY - Amazon Turk Live mode - jump directly to game
            each role in requiredRoles
              h4
                a(href='/#{gametype}/advanced/#{role}') PLAY - Jump directly to game as #{role}
      
      hr
      footer
        p Â© Company 2013
