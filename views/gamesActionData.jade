extends shared

block content
  br 
  br
  div.container
   
   h1 User Info
   br
   div.scrol
     table(class="table table-striped table-bordered")
       tr
         td Game Id
         td User Id
         td Gender
         td Age
         td Education
         td Field
         td Birth Country
         td browser Type
         td Game Type
         td Role
         td Game Id
         td Assisnment Id
         td Hit Id
         td Worker Id
         td Data Status
         td Completed
       - if (questionnaireList !== undefined)
         each questionnaire in questionnaireList 
          tr
            td #{questionnaire.PartitionKey}
            td #{questionnaire.RowKey}
            td #{questionnaire.gender}
            td #{questionnaire.age}
            td #{questionnaire.education}
            td #{questionnaire.field}
            td #{questionnaire.birth_country}
            td #{questionnaire.browserType}
            td #{questionnaire.gametype}
            td #{questionnaire.role}
            td #{questionnaire.gameid} 
            td #{questionnaire.assisnmentid}
            td #{questionnaire.hitid}
            td #{questionnaire.workerid}
            td #{questionnaire.datastatus}
            td #{questionnaire.completed}

   br
   h1 Post Questionnaire
   br
   div.scrol
     table(class="table table-striped table-bordered")
       tr
         td Role
         td Computer Program
         td Fairness
         td Feedback
         td Happy
         td Instruction
         td Other Gender
         td Same Country
         td Type
       - if (questionnaireList !== undefined)
         each questionnaire in questionnaireList  
          tr
            td #{questionnaire.role}
            td #{questionnaire.computer_program}
            td #{questionnaire.fairness}
            td #{questionnaire.feedback}
            td #{questionnaire.happy}
            td #{questionnaire.instruction}
            td #{questionnaire.otherGender}
            td #{questionnaire.same_country}
            td #{questionnaire.type}
   br
   h1 Game Action List
   br
   div.scrol
     table(class="table table-striped table-bordered")
       tr
         td Game Id
         td Action's Number
         td Timestamp
         td Action
         td Remaining Time
         td Role
         td User Id
         td Issue
         td Value
       - if (GameActionList !== undefined) 
         each GameAction in GameActionList
          tr
            td #{GameAction.PartitionKey}
            td #{GameAction.RowKey}
            td #{GameAction.Timestamp}
            td #{GameAction.action}
            td #{GameAction.remainingTime}
            td #{GameAction.role}
            td #{GameAction.userid}
            td #{GameAction.issue}
            td #{GameAction.value}
            
   br
   h1 Final Result 
   br
   div.scrol
     table(class="table table-striped table-bordered")
         tr
           td Game Id
           td User Id
           td Timestamp
           td Job Description
           td Leased Car
           td Pension Fond
           td Promotion Posibilities
           td Salary
           td Working Hours
           td Time From Start
           td Turns From Start
           td Utility Without Discount
           td Utility With Discount
         - if (FinalResultList !== undefined) 
           each FinalResult in FinalResultList
            tr
              td #{FinalResult.PartitionKey}
              td #{FinalResult.RowKey}
              td #{FinalResult.Timestamp}
              td #{FinalResult.JobDescription}
              td #{FinalResult.LeasedCar}
              td #{FinalResult.PensionFond}
              td #{FinalResult.PromotionPosibilities}
              td #{FinalResult.Salary}
              td #{FinalResult.WorkingHours}
              td #{FinalResult.timeFromStart}
              td #{FinalResult.turnsFromStart}
              td #{FinalResult.utilityWithoutDiscount}
              td #{FinalResult.utilityWithDiscount}       
